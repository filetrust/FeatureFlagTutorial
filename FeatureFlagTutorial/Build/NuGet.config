<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <packageSources>
    <add key="GlasswallPackages" value="https://glasswall.pkgs.visualstudio.com/_packaging/GlasswallPackages/nuget/v3/index.json" />
    <add key="dotnet-core" value="https://www.nuget.org/api/v2/" />
  </packageSources>
  <packageSourceCredentials>
    <GlasswallPackages>
      <add key="Username" value="${NuGetUsername}" />
      <add key="ClearTextPassword" value="${NuGetPassword}" />
    </GlasswallPackages>
  </packageSourceCredentials>
</configuration>